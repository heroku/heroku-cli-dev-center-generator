{
  "name": "@heroku/heroku-cli-dev-center-generator",
  "description": "Generates a README for a Heroku plugin",
  "version": "1.1.3",
  "author": "Jeff Dickey (@dickeyxxx)",
  "bin": {
    "heroku-cli-dev-center-generator": "./run.js"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "remark-cli": "^4.0.0",
    "remark-heroku-bash-dollar": "^1.0.2",
    "remark-heroku-command-headings-inline-code": "^1.0.4",
    "remark-heroku-example-command": "^1.0.5",
    "remark-heroku-sample-code": "^1.0.3",
    "remark-heroku-topic-overview": "^1.0.3",
    "remark-lint": "^6.0.0",
    "remark-lint-are-links-valid": "^1.0.3",
    "remark-preset-lint-recommended": "^3.0.0"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      "remark-heroku-example-command"
    ]
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cli-engine": "3.5.58",
    "heroku-apps": "^2.4.2",
    "heroku-certs": "^1.1.40",
    "heroku-ci": "^1.8.2",
    "heroku-cli-addons": "^1.2.22",
    "heroku-cli-oauth": "^2.0.13",
    "heroku-cli-status": "4.x",
    "heroku-fork": "^4.1.24",
    "heroku-git": "^2.5.18",
    "heroku-local": "^5.1.17",
    "heroku-orgs": "^1.6.3",
    "heroku-pg": "^2.2.4",
    "heroku-pipelines": "^2.1.2",
    "heroku-ps-exec": "^1.0.6",
    "heroku-run": "^3.4.12",
    "heroku-spaces": "^2.9.4",
    "jest": "^20.0.4",
    "standard": "10.0.2"
  },
  "files": [
    "run.js",
    "src"
  ],
  "license": "ISC",
  "main": "run.js",
  "peerDependencies": {
    "cli-engine": "*",
    "heroku-apps": "*",
    "heroku-certs": "*",
    "heroku-ci": "*",
    "heroku-cli-addons": "*",
    "heroku-cli-oauth": "*",
    "heroku-cli-status": "*",
    "heroku-fork": "*",
    "heroku-git": "*",
    "heroku-local": "*",
    "heroku-orgs": "*",
    "heroku-pg": "*",
    "heroku-pipelines": "*",
    "heroku-ps-exec": "*",
    "heroku-run": "*",
    "heroku-spaces": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/heroku/heroku-cli-dev-center-generator.git"
  },
  "scripts": {
    "test": "jest && standard",
    "generate": "./run.js > generated_docs.md",
    "lint": "remark generated_docs.md"
  },
  "standard": {
    "env": "jest"
  }
}
