{
  "name": "@heroku-cli/dev-center-generator",
  "description": "Generates dev center documentation for CLI and plugins",
  "version": "2.0.1",
  "author": "Heroku",
  "bugs": "https://github.com/heroku/heroku-cli-dev-center-generator/issues",
  "dependencies": {
    "@heroku-cli/command": "^8.0.5",
    "@oclif/command": "1",
    "@oclif/config": "1",
    "cli-ux": "^3.4.1",
    "lodash": "^4.17.5",
    "tslib": "1"
  },
  "devDependencies": {
    "@heroku-cli/tslint": "^1.1.2",
    "@oclif/dev-cli": "1",
    "@oclif/plugin-help": "1",
    "@oclif/test": "1",
    "@oclif/tslint": "1",
    "@types/ansi-styles": "^2.0.30",
    "@types/chai": "4",
    "@types/lodash": "^4.14.107",
    "@types/luxon": "^0.5.1",
    "@types/mocha": "5",
    "@types/node": "9",
    "@types/supports-color": "^5.3.0",
    "chai": "4",
    "globby": "8",
    "husky": "^0.14.3",
    "mocha": "5",
    "nyc": "11",
    "ts-node": "10.9.2",
    "tslint": "5",
    "typescript": "2.8"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "files": [
    "/.oclif.manifest.json",
    "lib"
  ],
  "homepage": "https://devcenter.heroku.com",
  "keywords": [
    "heroku-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "topics": {
      "devcenter": {
        "description": "dev center markdown commands"
      }
    },
    "commands": "./lib/commands",
    "bin": "heroku",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "heroku/heroku-cli-dev-center-generator",
  "scripts": {
    "postpack": "rm -f .oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "pretest": "tsc",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
